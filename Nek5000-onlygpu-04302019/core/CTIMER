      common /ctime2/ tpres,thmhz,tgop ,tgop1,tdott,tbsol,tbso2
      common /ctime2/ tsett,tslvb,tusbc,tddsl,tcrsl,tdsmx,tdsmn
      common /ctime2/ tgsmn,tgsmx,teslv,tbbbb,tcccc,tdddd,teeee
      common /ctime2/ tvdss,tschw,tadvc,tspro,tgop_sync,tsyc
      common /ctime2/ twal,tgp2,tcvf
!
      integer nmxmf,nmxms,ndsum,naxhm,ncopy,ninvc,ninv3
      common /itimer/ nmxmf,nmxms,ndsum,naxhm,ncopy,ninvc,ninv3
!
      integer         nsolv,ngsum,ndsnd,ndadd,ncdtp,nmltd,nprep
      integer         npres,nhmhz,ngop ,ngop1,ndott,nbsol,nbso2
      integer         nsett,nslvb,nusbc,nddsl,ncrsl,ndsmx,ndsmn
      integer         ngsmn,ngsmx,neslv,nbbbb,ncccc,ndddd,neeee
      integer         nvdss,nadvc,nspro,ngop_sync,nsyc,nwal,ngp2
      integer         ncvf
      common /itime2/ nsolv,ngsum,ndsnd,ndadd,ncdtp,nmltd,nprep
      common /itime2/ npres,nhmhz,ngop ,ngop1,ndott,nbsol,nbso2
      common /itime2/ nsett,nslvb,nusbc,nddsl,ncrsl,ndsmx,ndsmn
      common /itime2/ ngsmn,ngsmx,neslv,nbbbb,ncccc,ndddd,neeee
      common /itime2/ nvdss,nadvc,nspro,ngop_sync,nsyc,nwal,ngp2
      common /itime2/ ncvf
!   
      real*8          pmxmf,pmxms,pdsum,paxhm,pcopy,pinvc,pinv3
      real*8          psolv,pgsum,pdsnd,pdadd,pcdtp,pmltd,pprep
      real*8          ppres,phmhz,pgop ,pgop1,pdott,pbsol,pbso2

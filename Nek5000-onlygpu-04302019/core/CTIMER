      real*8          pgsmn,pgsmx,peslv,pbbbb,pcccc,pdddd,peeee
      real*8          pvdss,pspro,pgop_sync,psyc,pwal,pgp2
      common /ptimer/ pmxmf,pmxms,pdsum,paxhm,pcopy,pinvc,pinv3

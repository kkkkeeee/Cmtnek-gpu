!
!     Mass matrix
!
      real bm1(lx1,ly1,lz1,lelt),bm2(lx2,ly2,lz2,lelv)
      real binvm1(lx1,ly1,lz1,lelv),bintm1(lx1,ly1,lz1,lelt)
      real bm2inv(lx2,ly2,lz2,lelt),baxm1(lx1,ly1,lz1,lelt)
      real bm1lag(lx1,ly1,lz1,lelt,lorder-1)
      real volvm1,volvm2,voltm1,voltm2
      real yinvm1(lx1,ly1,lz1,lelt)
      real binvdg(lx1*ly1*lz1,lelt)
      common /mass/ bm1,bm2,binvm1,bintm1,bm2inv,baxm1,bm1lag
      common /mass/ volvm1,volvm2,voltm1,voltm2,yinvm1,binvdg
